[{"name": "\u0420\u043e\u0441\u0442\u0438\u0441\u043b\u0430\u0432", "text": "\u041f\u0420\u0438\u0432\u0435\u0442 \u043e\u0442\u043b\u0438\u0447\u043d\u044b\u0439 \u0444\u0438\u043b\u044c\u0442\u0440"}, {"name": "\u041b\u0443\u043f\u0430!", "text": "\u041f\u0443\u043f\u0430, \u0433\u0434\u0435 \u043c\u043e\u044f \u0437\u0430\u0440\u043f\u043b\u0430\u0442\u0430!?"}, {"name": "\u041f\u0443\u043f\u0430", "text": "<p><strong>\u041b\u0443\u043f\u0430,</strong> \u0442\u044b \u0443\u0436\u0435 \u043f\u043e\u043b\u0443\u0447\u0438\u043b\u0430 \u0437\u0430\u0440\u043f\u043b\u0430\u0442\u0443!</p>\r\n<p style=\"padding-left: 30px;\">\u0417\u0430 \u043c\u0435\u043d\u044f!!</p>"}]